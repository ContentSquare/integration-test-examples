{
    "compilerOptions": {
        "moduleResolution": "node",
        "target": "es2020",
        "module": "es2020",
        "strict": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "jsx": "preserve",
        "types": ["node", "svelte"],
        "baseUrl": ".",
        "paths": {
            "src/*": ["src/*"]
        }
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }
  